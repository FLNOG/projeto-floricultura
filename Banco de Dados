create database loja;
use loja;

drop table produto;

-- Criando a tabela PRODUTO
CREATE TABLE PRODUTO (
    idProduto INT NOT NULL PRIMARY KEY AUTO_INCREMENT,
    descricao VARCHAR(200) NOT NULL,
    detalhes VARCHAR(500),
    quantidade INT,
    preco DECIMAL(10, 2),
    imagem TEXT
);

-- alter table produto modify column preco decimal(10,2);

-- Verificação de dados na tabela PRODUTO
SELECT * FROM PRODUTO;

-- Criando a tabela CLIENTE
CREATE TABLE CLIENTE (
    idCliente INT NOT NULL PRIMARY KEY AUTO_INCREMENT,
    nome VARCHAR(200),
    cpfCnpj VARCHAR(20),
    tipo VARCHAR(1),
    nascimento VARCHAR(10),
    telefone VARCHAR(15),
    senha VARCHAR(20),
    email VARCHAR(50),
    
	logradouro VARCHAR(100),
    numero VARCHAR(10),
    cep VARCHAR(9),
    bairro VARCHAR(100),
    cidade VARCHAR(100),
    uf VARCHAR(2),
    apelido VARCHAR(100),
    complemento VARCHAR(100)
);

-- ALTER TABLE CLIENTE ADD complemento VARCHAR(100);
-- ALTER TABLE CLIENTE MODIFY COLUMN uf VARCHAR(4);

-- Inserindo um registro de exemplo na tabela CLIENTE
INSERT INTO CLIENTE (nome, cpfCnpj, telefone) VALUES ('Felipe', '4343434', '11992928213');
INSERT INTO CLIENTE (NOME, CPFCNPJ, TIPO, TELEFONE, SENHA, EMAIL) VALUES ('Felipe Nog', '1234567890', '1', '11992108640', '123', 'felipe@adm');
-- Verificação de dados na tabela CLIENTE
SELECT * FROM CLIENTE;

-- Criando a tabela VENDA
CREATE TABLE VENDA (
    idVenda INT PRIMARY KEY AUTO_INCREMENT,
    vlrTotal DECIMAL(10, 2),
    dataVenda DATE,
    idCliente INT,
    CONSTRAINT fk_idCliente FOREIGN KEY (idCliente) REFERENCES CLIENTE(idCliente)
);

-- Verificação de dados na tabela VENDA
SELECT * FROM VENDA;

-- Criando a tabela ITEMVENDA
CREATE TABLE ITEMVENDA (
    idItemVenda INT PRIMARY KEY AUTO_INCREMENT,
    idVenda INT,
    idProduto INT,
    qtdItens INT NOT NULL,
    vlrProduto DECIMAL(10, 2) NOT NULL,
    CONSTRAINT fk_idVenda FOREIGN KEY (idVenda) REFERENCES VENDA(idVenda),
    CONSTRAINT fk_idProduto FOREIGN KEY (idProduto) REFERENCES PRODUTO(idProduto)
);

-- Verificação de dados na tabela ITEMVENDA
SELECT * FROM ITEMVENDA;

